%h1= 'Java portfolio items'

- @java_item.each do |p|
    = p.title
    %br
    = p.subtitle
    %br
    = p.body
    %b
    = image_tag p.thumb_image
    %br
    = link_to "link to portfolio", portfolio_show_path(p)
    = link_to "edit portfolio", edit_portfolio_path(p)
    = link_to 'delete portfolio', portfolio_path(p), method: :delete
    %br
    %br